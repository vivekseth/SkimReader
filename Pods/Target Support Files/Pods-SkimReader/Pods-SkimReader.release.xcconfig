OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/KFEpubKit" -isystem "${PODS_ROOT}/Headers/Public/KissXML" -isystem "${PODS_ROOT}/Headers/Public/SSZipArchive" -isystem "${PODS_ROOT}/Headers/Public/VSSpritz"
OTHER_LDFLAGS = -ObjC -l"Pods-SkimReader-KFEpubKit" -l"Pods-SkimReader-KissXML" -l"Pods-SkimReader-SSZipArchive" -l"Pods-SkimReader-VSSpritz" -l"xml2" -l"z" -framework "Foundation"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods